{
  "version": 2,
  "name": "next-now2-tests",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { 
      "src": "/.*",
      "headers": {"cache-control": "s-maxage=31536000, max-age=31536000, immutable"},
      "continue": true
    },
    
    {
      "src": "/a-route/", 
      "headers": {"cache-control": "s-maxage=1800, max-age=1800, immutable"},
      "dest": "/"
    },
    
    { "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+", "headers": { "cache-control": "immutable" } }

  ]
}